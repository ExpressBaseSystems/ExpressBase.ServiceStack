@{

}

@model List<Displaydata>



<table border="1">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Middle Name</th>
    </tr>
    @foreach (var dispdata in Model)
    {
       
        <tr>
            <td>@dispdata.FirstName</td>
            <td>@dispdata.LastName</td>
            <td>@dispdata.MiddleName</td>
            <td>
               
               <a href="~/home/registerview?id=@dispdata.id">Edit</a>
            </td>
        </tr>
    }
</table>